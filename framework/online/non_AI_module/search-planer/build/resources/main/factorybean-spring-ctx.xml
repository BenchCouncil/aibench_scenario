<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="excellentProductSearchClient" class="org.benchcouncil.aibench.SearchClient">
        <constructor-arg name="host" value="elasticsearch"/>
        <constructor-arg name="port" value="9200"/>
        <constructor-arg name="schema" value="http"/>
    </bean>
    <bean id="goodProductSearchClient" class="org.benchcouncil.aibench.SearchClient">
        <constructor-arg name="host" value="elasticsearch"/>
        <constructor-arg name="port" value="9200"/>
        <constructor-arg name="schema" value="http"/>
    </bean>
    <bean id="badProductSearchClient" class="org.benchcouncil.aibench.SearchClient">
        <constructor-arg name="host" value="elasticsearch"/>
        <constructor-arg name="port" value="9200"/>
        <constructor-arg name="schema" value="http"/>
    </bean>
    <bean id="rankerClient" class="org.benchcouncil.aibench.SearchClient">
        <constructor-arg name="host" value="elasticsearch"/>
        <constructor-arg name="port" value="9200"/>
        <constructor-arg name="schema" value="http"/>
    </bean>
    <bean id="productClient" class="org.benchcouncil.aibench.SearchClient">
        <constructor-arg name="host" value="elasticsearch"/>
        <constructor-arg name="port" value="9200"/>
        <constructor-arg name="schema" value="http"/>
    </bean>
</beans>
